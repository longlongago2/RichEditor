{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0"
  ],
  "env": {
    "production": {
      "presets": [
        "react",
        "es2015",
        "stage-0"
      ],
      "plugins": [
        [
          "babel-plugin-webpack-loaders",
          {
            "config": "./lib.webpack.config.js",
            "verbose": false
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}